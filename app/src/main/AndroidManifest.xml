<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.myapplication2.app">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/logo"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
        <activity
                android:name=".ButtonsActivity"
                android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".ScanActivity"
                android:label="@string/title_activity_scan">
        </activity>
        <activity
                android:name=".ConfirmMedActivity"
                android:label="@string/title_activity_place_med">
        </activity>
        <activity
                android:name=".StatusActivity"
                android:label="@string/title_activity_details">
        </activity>
        <activity
                android:name=".PlaceMedActivity"
                android:label="@string/title_activity_place_med">
        </activity>
        <activity
                android:name=".TabDetailsActivity"
                android:label="@string/title_activity_tab_details">
        </activity>
        <activity
                android:name=".InfoActivity"
                android:label="@string/title_activity_info">
        </activity>
    </application>

</manifest>
